@{
    ViewData["Title"] = "Claims";
}
<h1>@ViewData["Title"]</h1>

<div class="jumbotron">
    <div>
        <ul>
            @foreach (var claim in ViewBag.Claims)
            {
                <li><b>@claim.Key:</b> @claim.Value</li>
            }
        </ul>
    </div>

    <a asp-area="AzureADB2C" asp-controller="Account" asp-action="EditProfile">Edit your profile</a>
</div>
